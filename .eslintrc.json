{
  "env": {
    "node": true
  },
  "extends": [
    "node",
    "eslint:recommended",
    "plugin:eslint-comments/recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended",
    "plugin:node/recommended",
    "plugin:promise/recommended",
    "plugin:import/recommended"
  ],
  "plugins": ["prettier", "promise"],
